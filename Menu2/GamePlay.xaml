<Window x:Class="Menu2.GamePlay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Menu2"
        mc:Ignorable="d"
        Title="GamePlay" Height="450" Width="800">
    <Grid>
        <Canvas x:Name="GameScreen" Focusable="True" KeyUp="KeyboardUp" KeyDown="KeyBoardDown" Margin="7,0,0,0">
            <Rectangle x:Name="Player" Fill="Red" Width="12" Height="14" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Top="179" Canvas.Left="350"/>

        </Canvas>
    </Grid>
</Window>

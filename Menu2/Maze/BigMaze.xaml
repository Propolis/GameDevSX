<Page x:Class="Menu2.Maze.BigMaze"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Menu2.Maze"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="BigMaze">

    <Grid x:Name="GameScreen" Background="Wheat">
        <Label HorizontalAlignment="Left" Margin="-2498,-2349,0,0" VerticalAlignment="Top" Height="2852" Width="3298">
            <Label.Background>
                <ImageBrush ImageSource="/Maze/BigMaze.jpg"/>
            </Label.Background>
        </Label>
        <Rectangle x:Name="Character" Margin="603,400,134,0">
            <Rectangle.Fill>
                <ImageBrush x:Name="ImagePlayer" ImageSource="/Maze/characterRight.png"/>
            </Rectangle.Fill>
        </Rectangle>
    </Grid>
</Page>
